{
  "vim_mode": true,
  "ui_font_size": 15,
  "buffer_font_size": 15,
  "terminal": {
    "dock": "bottom",
    "font_size": 15,
    "line_height": "standard",
    "copy_on_select": true
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "agent": {
    "dock": "right",
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "version": "2"
  },
  "notification_panel": {
    "dock": "left"
  },
  "tab_bar": {
    "show_tab_bar_buttons": false,
    "show_nav_history_buttons": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "git_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  }
}
